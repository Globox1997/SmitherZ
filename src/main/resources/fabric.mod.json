{
	"schemaVersion": 1,
	"id": "smitherz",
	"version": "${version}",
	"name": "SmitherZ",
	"description": "Enhance your tools with upgrades",
	"authors": [
		"Globox_Z"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/smitherz/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.smitherz.SmitherzMain"
		],
		"client": [
			"net.smitherz.SmitherzClient"
		],
		"modmenu": [
			"net.smitherz.config.ModMenuIntegration"
		]
	},
	"mixins": [
		"smitherz.mixins.json"
	],
	"depends": {
		"java": ">=17",
		"minecraft": ">=${minecraft_version}",
		"fabric-api": ">=${fabric_version}",
		"fabricloader": ">=${loader_version}",
		"cloth-config2": ">=${cloth_config_version}",
		"libz": ">=${libz_version}",
		"autotag-convention": ">=${autotag_version}",
		"advanced_runtime_resource_pack": ">=${arrp_version}"
	},
	"suggests": {
		"another-mod": "*"
	},
	"custom": {
		"mc-publish": {
			"modrinth": "X",
			"curseforge": 0
		}
	}
}